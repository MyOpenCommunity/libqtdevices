	<css>
WindowContainer, PageContainer, Page, QLabel, QLCDNumber, Version, QStackedWidget { background-color:#1E1E1E; color:#D8D8D8; }
BtButton { border: 0px; }
QWidget[noStyle="true"] {background-color:#FFFFFF;color:#000000;}
QWidget[noStyle="true"] QWidget {background-color:#FFFFFF;color:#000000;}
QWidget[SecondFgColor="true"] {color:#6FB2FF;}
QWidget[SecondFgColor="true"] QWidget {color:#6FB2FF;}
QWidget[ReversedColor="true"] {color:#1E1E1E;background-color:#D8D8D8;}
TextOnImageLabel { qproperty-text_color:#D8D8D8; }
EnergyGraph {
	qproperty-primary_color: #1449C8;
	qproperty-secondary_color: #3587FF;
	qproperty-border_color: blue;
	qproperty-axis_color: white;
	qproperty-text_color: white;
}
EnergyTableContent {
	qproperty-border_color: #41474d;
	qproperty-heading_color: #3587ff;
	qproperty-odd_row_color: #41474d;
	qproperty-even_row_color: #2e3236;
	qproperty-text_color: #d8d8d8;
}
EnlargeTransition {
	qproperty-border_color: #c0c0c0;
}
WaitLabel { background-color: #000000; background-image: url(cfg/extra/1/skin/loading.png); background-position: center center; background-repeat: no-repeat; }
KeypadWithState QLabel[ActiveState="true"] { background-color:white; color:black; }
FileSelector > QLabel[Loading="true"] { background-color: #000000; background-image: url(cfg/extra/1/skin/loading.png); background-position: center center; background-repeat: no-repeat; }
