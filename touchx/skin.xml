QLabel, QLCDNumber { color:#FFFFFF; }
BtButton { border: 0px; }
Window { background-color: #000000; }
KeypadWindow, VCTCallPrivate--VCTCallWindow { background-image: url(cfg/extra/1/skin/layoutback.png); }
QWidget[noStyle="true"] {background-color:#FFFFFF;color:#000000;}
QWidget[noStyle="true"] QWidget {background-color:#FFFFFF;color:#000000;}
QWidget[SecondFgColor="true"] {color:#6FB2FF;}
QWidget[SecondFgColor="true"] QWidget {color:#6FB2FF;}
QWidget[ReversedColor="true"] {color:#1E1E1E;background-color:#D8D8D8;}
TextOnImageLabel { qproperty-text_color:#D8D8D8; }
EnergyGraph { qproperty-primary_color:#1449C8; qproperty-secondary_color:#3587FF; qproperty-border_color: blue;}
KeypadWithState QLabel[ActiveState="true"] { background-color:white; color:black; }
HeaderInfo { background-image: url(cfg/extra/1/skin/topinfobar.png); }
HeaderNavigationBar { background-image: url(cfg/extra/1/skin/topnavigationbar.png); }
Page { background-image: url(cfg/extra/1/skin/layoutback.png); }
QWidget[Favorites="true"] { background-image: url(cfg/extra/1/skin/preferiti.png); }
HeaderLogo { background-image: url(cfg/extra/1/skin/topstatusbar_3.png); }
QWidget[VerticalSeparator="true"] { background-image: url(cfg/extra/1/skin/sepvert.png); background-position: top center; background-repeat: no-repeat; }
ItemList > QWidget { background-image: url(cfg/extra/1/skin/multimediale/backfileaudio.png); }
RadioInfo { background-image: url(cfg/extra/1/skin/diffusione/displaybig.png); }
FileSelector > QLabel[Loading="true"] { background-color: #000000; background-image: url(cfg/extra/1/skin/clessidra.png); background-position: center center; background-repeat: no-repeat; }
FeedItemWidget > QTextEdit { background-color: #c0c0c0; color: #000000; }
MessagesListPage QLabel[UnreadMessage="true"] { color:#FF0000; }
MessagesListPage QLabel[UnreadMessage="false"] { color:#6FB2FF; }
MessagePage QLabel#Date {  background-color: gray; }
MessagePage QLabel#Text { background-color: #000000; }
	</css>
